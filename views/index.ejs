
<h3 class="col-span-2 text-2xl mb-3 mt-2 ml-5">Liste des Ordinateurs</h3>
<div class="h-full flex p-2">
    <div class="overflow-x-auto">
        <table class="min-w-full divide-y divide-gray-200">
            <thead class="border-b bg-orange-400">
            <tr>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">username</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">cuid</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">status</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">direction</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">site</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">hostname</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">serial Number</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Version OS</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">type</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">model</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">sac</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">clavier</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">souris</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">ecran</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">imprimente</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">token</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">cable Securité</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">filtre Confidentialité</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Annee Attribution</th>
                <th  scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"></th>
            </tr>
            </thead>
            <tbody class="bg-white divide-y divide-gray-200">
            <% if (computers) { %>
                <% computers.forEach(function(computer) {%>
                    <tr class="border-b">
                        <td  class="px-6 py-4 whitespace-nowrap"><%= computer.username %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.cuid %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.status %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.direction %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.site %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.hostname %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.serialNumber %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.osVersion %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.type %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.model %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.sac %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.clavier %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.souris %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.ecran %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.imprimente %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.token %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.cableSecure %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.filtreSecure %></td>
                        <td class="px-6 py-4 whitespace-nowrap"><%= computer.anneeAttr?.toLocaleDateString("en-US" , {year: 'numeric'}) %></td>
                        <td class="px-6 py-4 whitespace-nowrap">
                            <a class="text-sm bg-red-400 text-white px-3 py-1 rounded shadow-lg" href="/<%=computer._id %>/delete">Supprimer</a>
                            <a class="text-sm bg-green-400 text-white px-3 py-1 rounded shadow-lg"  href="/<%=computer._id %>/edit">Modifier</a>
                        </td>
                    </tr>
                <% });%>
            <% }%>
            </tbody>
        </table>
    </div>
</div>